{
    "name": "jupyterlab_globus",
    "version": "0.2.1",
    "description": "Incorporates Globus functionality",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/gneezyn/jupyterlab_globus",
    "bugs": {
        "url": "https://github.com/gneezyn/jupyterlab_globus/issues"
    },
    "license": "BSD-3-Clause",
    "author": "Juan David Garrido",
    "contributors": [
        "Netta Gneezy"
    ],
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/gneezyn/jupyterlab_globus.git"
    },
    "scripts": {
        "build": "tsc && npm run extension",
        "extension": "npm run-script extension_script.sh",
        "test": "mocha -r ts-node/register -r ignore-styles -r jsdom-global/register src/**/*.spec.ts",
        "clean": "rimraf lib",
        "watch": "tsc -w"
    },
    "dependencies": {
        "@jupyterlab/application": "^0.19.1",
        "@jupyterlab/apputils": "^0.19.1",
        "@jupyterlab/filebrowser": "^0.19.4",
        "@jupyterlab/services": "^3.2.1",
        "@jupyterlab/terminal": "^0.19.1",
        "@types/crypto-js": "^3.1.43",
        "@types/jquery": "^3.3.29",
        "crypto-js": "^3.1.9-1",
        "jquery": "^3.4.1",
        "json-formatter-js": "^2.2.1",
        "marked": "^0.6.2",
        "rxjs": "^6.5.2",
        "tar": "^4.4.8"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.0.10",
        "chai": "^4.2.0",
        "ignore-styles": "^5.0.1",
        "jsdom": "^15.1.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^6.1.4",
        "rimraf": "^2.6.3",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.5"
    },
    "jupyterlab": {
        "extension": true
    }
}
